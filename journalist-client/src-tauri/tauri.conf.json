{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Sentinel DEV",
  "version": "0.1.0",
  "identifier": "com.theguardian.coverdrop-journalist-client.DEV",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Sentinel DEV",
        "width": 800,
        "height": 600,
        "minWidth": 780,
        "minHeight": 400
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/dev/32x32.png",
      "icons/dev/128x128.png",
      "icons/dev/128x128@2x.png",
      "icons/dev/icon.icns",
      "icons/dev/icon.ico"
    ]
  }
}
